{
    "name": "hozien/laravel-awesome-uploader",
    "description": "A customizable and pluggable file uploader for Laravel.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Hozien"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "illuminate/http": "^9.0 || ^10.0 || ^11.0",
        "illuminate/filesystem": "^9.0 || ^10.0 || ^11.0",
        "illuminate/routing": "^9.0 || ^10.0 || ^11.0",
        "illuminate/view": "^9.0 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Hozien\\Uploader\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hozien\\Uploader\\UploaderServiceProvider"
            ],
            "aliases": {
                "Uploader": "Hozien\\Uploader\\Facades\\Uploader"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 